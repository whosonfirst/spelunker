CREATE TABLE {{ .Name }} (
    id VARCHAR(255) NOT NULL,
    parent_id BIGINT,
    name VARCHAR(255),
    placetype VARCHAR(48),
    inception VARCHAR(64),
    cessation VARCHAR(64),
    country VARCHAR(4),
    repo VARCHAR(255),
    latitude FLOAT,
    longitude FLOAT,
    min_latitude FLOAT,
    min_longitude FLOAT,
    max_latitude FLOAT,
    max_longitude FLOAT,
    is_current TINYINT(1),
    is_deprecated TINYINT(1),
    is_ceased TINYINT(1),
    is_superseded TINYINT(1),
    is_superseding TINYINT(1),
    superseded_by TEXT,
    supersedes TEXT,
    belongsto TEXT,
    is_alt TINYINT(1),
    alt_label VARCHAR(255),
    lastmodified BIGINT,
    PRIMARY KEY `{{ .Name }}_by_id`  (id, alt_label),
    KEY `{{ .Name }}_by_lastmod`  (lastmodified),
    KEY `{{ .Name }}_by_parent`  (parent_id, is_current, lastmodified),
    KEY `{{ .Name }}_by_placetype`  (placetype, is_current, lastmodified),
    KEY `{{ .Name }}_by_country`  (country, placetype, is_current, lastmodified),
    KEY `{{ .Name }}_by_name`  (name, placetype, is_current, lastmodified),
    KEY `{{ .Name }}_by_centroid`  (latitude, longitude, is_current, lastmodified),
    KEY `{{ .Name }}_by_bbox`  (min_latitude, min_longitude, max_latitude, max_longitude, placetype, is_current, lastmodified),
    KEY `{{ .Name }}_by_repo`  (repo, lastmodified),
    KEY `{{ .Name }}_by_current`  (is_current, lastmodified),
    KEY `{{ .Name }}_by_deprecated`  (is_deprecated, lastmodified),
    KEY `{{ .Name }}_by_ceased`  (is_ceased, lastmodified),
    KEY `{{ .Name }}_by_superseded`  (is_superseded, lastmodified),
    KEY `{{ .Name }}_by_superseding`  (is_superseding, lastmodified),
    KEY `{{ .Name }}_obsolete`  (is_deprecated, is_superseded)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;
